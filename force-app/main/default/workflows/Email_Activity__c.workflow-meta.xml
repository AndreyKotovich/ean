        <actions>
            <name>Cancel_group_event_registration_request</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Email_sent</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Email_Activity__c.Type__c</field>
            <operation>equals</operation>
            <value>Group Registration Cancellation</value>
        </criteriaItems>
        <criteriaItems>
            <field>Email_Activity__c.Email_Sent__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>

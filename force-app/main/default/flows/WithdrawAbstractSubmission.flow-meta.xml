<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <decisions>
        <name>Event_Started</name>
        <label>Event Started?</label>
        <locationX>515</locationX>
        <locationY>566</locationY>
        <defaultConnector>
            <targetReference>AbstractCannotBeWithdrawnScreen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>NotStarted</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GetEventDate.Start_Time__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDateTime</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Confirm_Abstract_Withdrawal_Screen</targetReference>
            </connector>
            <label>NotStarted</label>
        </rules>
    </decisions>
    <decisions>
        <name>Stage_Submitted</name>
        <label>Stage = Submitted?</label>
        <locationX>669</locationX>
        <locationY>446</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Submitted</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Submitted</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>In Review</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Event_Started</targetReference>
            </connector>
            <label>Submitted</label>
        </rules>
        <rules>
            <name>Not_Submitted</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Draft</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Close</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AbstractCannotBeWithdrawnScreen</targetReference>
            </connector>
            <label>Not Submitted</label>
        </rules>
    </decisions>
    <decisions>
        <name>Withdrawal_Confirmed</name>
        <label>Withdrawal Confirmed?</label>
        <locationX>515</locationX>
        <locationY>902</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Confimed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CorfirmAbstractWithdrawalToggle.value</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdateStage</targetReference>
            </connector>
            <label>Confimed</label>
        </rules>
    </decisions>

    <screens>
        <name>AbstractCannotBeWithdrawnScreen</name>
        <label>Abstract cannot be withdrawn screen</label>
        <locationX>823</locationX>
        <locationY>566</locationY>
        <allowBack>false</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <fields>
            <name>AbstractCannotBeWithdrawnText</name>
            <fieldText>&lt;p&gt;Your abstract cannot be withdrawn.&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>AbstractStageError</name>
            <fieldText>&lt;p&gt;Abstracts can only be withdrawn if they have been submitted or are in review.&lt;/p&gt;&lt;p&gt;It is in following stage:&lt;/p&gt;&lt;p&gt;{!GetAbstractStage.Stage__c}&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                    <operator>NotEqualTo</operator>
                    <rightValue>
                        <stringValue>Submitted</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>GetAbstractStage.Stage__c</leftValueReference>
                    <operator>NotEqualTo</operator>
                    <rightValue>
                        <stringValue>In Review</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>EventStartTimeError</name>
            <fieldText>&lt;p&gt;Abstracts can only be withdrawn before the event start time. &lt;/p&gt;&lt;p&gt;The event started at {!GetEventDate.Start_Time__c}&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
            <visibilityRule>
                <conditionLogic>and</conditionLogic>
                <conditions>
                    <leftValueReference>GetEventDate.Start_Time__c</leftValueReference>
                    <operator>LessThanOrEqualTo</operator>
                    <rightValue>
                        <elementReference>$Flow.CurrentDateTime</elementReference>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <showFooter>true</showFooter>
        <showHeader>true</showHeader>
    </screens>
    <screens>
